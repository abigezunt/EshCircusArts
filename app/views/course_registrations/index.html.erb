<h1>Listing course_registrations</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Course</th>
      <th>Paid</th>
      <th>Timestamps</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @course_registrations.each do |course_registration| %>
      <tr>
        <td><%= course_registration.user %></td>
        <td><%= course_registration.course %></td>
        <td><%= course_registration.paid %></td>
        <td><%= course_registration.timestamps %></td>
        <td><%= link_to 'Show', course_registration %></td>
        <td><%= link_to 'Edit', edit_course_registration_path(course_registration) %></td>
        <td><%= link_to 'Destroy', course_registration, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Registration', new_course_registration_path %>
